<!DOCTYPE html>
<html>
<head>
  <title>Zapauls</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
    <% form_tag('/search', :method => 'get') do %>
      <%= text_field_tag :search, params[:search], :id => 'search_field' %>
       <%= submit_tag "Search", :name => nil %> 
    <% end %> 


<%= yield %>

</body>
</html>
